{
  "name": "BooksList",
  "version": "1.0.0",
  "author": "Javier Cobos",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Knskan3/books-list.git"
  },
  "scripts": {
    "local": "cross-env NODE_ENV=development webpack-dev-server --inline --content-base public --history-api-fallback --config webpack/dev.config.js",
    "build": "cross-env NODE_ENV=production webpack --config webpack/build.config.js",
    "start": "yarn build && yarn server",
    "lint": "eslint ./src --ignore-path .gitignore",
    "test": "yarn lint && yarn jest",
    "jest": "jest --verbose --coverage",
    "jest:clean": "yarn run jest -u --verbose --coverage",
    "clean": "rm -rf ./coverage ./build",
    "server": "babel-node ./src/server/index.js",
    "generate": "babel-node ./src/book-generator/index.js"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.3",
    "babel-core": "^6.26.0",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^21.2.0",
    "babel-loader": "^7.1.2",
    "babel-preset-react": "^6.24.1",
    "caniuse-lite": "^1.0.30000738",
    "case-sensitive-paths-webpack-plugin": "^2.1.1",
    "clean-webpack-plugin": "^0.1.16",
    "cross-env": "^5.0.5",
    "css-loader": "^0.28.7",
    "enzyme-adapter-react-15": "^1.0.0",
    "eslint": "^4.7.2",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jest": "^21.1.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.3.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "html-webpack-plugin": "^2.30.1",
    "identity-obj-proxy": "^3.0.0",
    "image-webpack-loader": "^3.4.2",
    "jest": "^21.1.0",
    "postcss-cssnext": "^3.0.2",
    "postcss-import": "^10.0.0",
    "postcss-loader": "^2.0.6",
    "regenerator-runtime": "^0.11.0",
    "style-loader": "^0.18.2",
    "stylelint": "^8.1.1",
    "stylelint-webpack-plugin": "^0.9.0",
    "webpack": "^3.5.5",
    "webpack-dev-server": "^2.7.1"
  },
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-regenerator": "^6.22.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-stage-0": "^6.22.0",
    "babel-register": "^6.26.0",
    "cors": "^2.8.4",
    "enzyme": "^3.0.0",
    "eslint-plugin-redux-saga": "^0.5.0",
    "express": "^4.15.5",
    "moment": "^2.18.1",
    "mongojs": "^2.4.1",
    "prop-types": "^15.6.0",
    "random-datetime": "^1.0.1",
    "random-name": "^0.1.2",
    "random-title": "^1.0.4",
    "react": "^15.6.1",
    "react-bootstrap": "^0.31.3",
    "react-dom": "^15.6.1",
    "react-hot-loader": "next",
    "react-redux": "^5.0.6",
    "react-test-renderer": "^15.6.2",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "redux-saga": "^0.15.6",
    "reselect": "^3.0.1",
    "uglifyjs-webpack-plugin": "^0.4.6"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js"
    }
  }
}
